<%= dt_account_nav %>

<div id="leftColW">
	<h2>My Groups</h2>

	<% if logged_in? %>
	<h3 class="title">Your Groups</h3>
	<% if current_user.groups.length > 0 -%>
	<ul id="group-list">  
	<%= render_partial_collection 'dt/memberships/group', current_user.groups if current_user.groups %>
	</ul>
	<% end -%>
	<p><small><%= link_to "create group", dt_new_group_path %></small></p>
	<% end %>
</div>
<div id="rightColN">
	
</div>
