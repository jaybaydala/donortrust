<% semantic_form_for([:iend, @team.campaign, @team]) do |f| %>
  <%= f.error_messages %>
  <% f.inputs "Team" do %>
    <%= f.input :name, :label => "Campaign Name" %>
    <%= f.input :description %>
    <%= f.input :goal %>
  <% end %>
  <% f.buttons do %>
    <%= f.commit_button %>
  <% end %>
<% end %>