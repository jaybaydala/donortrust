<h2>Invite Friends</h2>
<div class="medium">
  <div id="fb-root"></div>
  <p>
  <input type="button" onclick="sendRequestViaMultiFriendSelector(); return false;" value="Invite Friends" />
  </p>

  <script>

    function sendRequestViaMultiFriendSelector() {
      FB.ui({method: 'send',
        name: 'Uend Poverty',
        description: 'UEnd Poverty provides gift options to empower people like you to end extreme poverty tailored to your interests.',
        link: 'http://www.uend.org/iend/users/new'
      });
    }

  </script>
</div>
